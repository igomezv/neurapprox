        "def RHS(Omega_i, lna, gamma=0):\n",
        "    x, y, z, H = Omega_i\n",
        "    #x, y, z = Omega_i\n",
        "    pi = 3*x + 4*y\n",
        "    return [x*(-3 + pi), y*(-4 + pi), z*pi, -0.5*H*pi]\n",
        "    #return [x*(-3 + pi), y*(-4 + pi), z*pi]\n",
        "\n",
        "def EDO(t,Om,H0):\n",
        "    #t,Or,Om,Ol=X\n",
        "    Or=0.0001\n",
        "    Ol=1-Or-Om\n",
        "    #H0 = 70.\n",
        "    y0 = [Om, Or, Ol, H0]\n",
        "    result = odeint(RHS, y0, t)\n",
        "    return result"
      ]
    },
